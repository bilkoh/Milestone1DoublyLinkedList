{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "CMake: build",
      "type": "shell",
      "command": "${config:cmake.cmakePath}",
      "args": [
        "--build",
        "${workspaceFolder}/out/build/prebuild",
        "--config",
        "Debug"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$msCompile"]
    }
  ]
}
